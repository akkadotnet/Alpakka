<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <AssemblyTitle>Akka.Streams.SNS</AssemblyTitle>
        <Description>Amazon SNS adapter for Akka.NET Streams</Description>
        <TargetFrameworks>$(NetStandardLibVersion);$(NetLibVersion)</TargetFrameworks>
        <PackageTags>$(AkkaPackageTags);Amazon;AWS;SNS</PackageTags>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Akka.Streams" Version="$(AkkaVersion)" />
        <PackageReference Include="AWSSDK.SimpleNotificationService" Version="3.7.300.41" />
    </ItemGroup>

</Project>
