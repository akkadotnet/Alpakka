<Project Sdk="Microsoft.NET.Sdk">
    <Import Project="..\..\..\common.props" />

    <PropertyGroup>
        <AssemblyTitle>Akka.Streams.SNS</AssemblyTitle>
        <Description>Amazon SNS adapter for Akka.NET Streams</Description>
        <TargetFramework>$(NetStandardLibVersion)</TargetFramework>
        <PackageTags>$(AkkaPackageTags);Amazon;AWS;SNS</PackageTags>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Akka.Streams" Version="$(AkkaVersion)" />
        <PackageReference Include="AWSSDK.SimpleNotificationService" Version="3.7.4.22" />
    </ItemGroup>

</Project>
