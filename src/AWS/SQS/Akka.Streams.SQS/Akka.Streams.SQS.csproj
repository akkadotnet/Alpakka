<Project Sdk="Microsoft.NET.Sdk">
    <Import Project="..\..\..\common.props" />

    <PropertyGroup>
        <AssemblyTitle>Akka.Streams.SQS</AssemblyTitle>
        <Description>Amazon SQS adapter for Akka.NET Streams</Description>
        <TargetFramework>$(NetStandardLibVersion)</TargetFramework>
        <PackageTags>$(AkkaPackageTags);Amazon;AWS;SQS</PackageTags>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Akka.Streams" Version="$(AkkaVersion)" />
        <PackageReference Include="AWSSDK.SQS" Version="3.7.0.44" />
    </ItemGroup>

</Project>
