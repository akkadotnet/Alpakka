<Project Sdk="Microsoft.NET.Sdk">
    <Import Project="..\..\common.props" />

    <PropertyGroup>
        <AssemblyTitle>Akka.Streams.Amqp</AssemblyTitle>
        <Description>AMQP adapter for Akka.NET Streams</Description>
        <TargetFramework>$(NetStandardLibVersion)</TargetFramework>
        <PackageTags>$(AkkaPackageTags);AMQP;RabbitMQ</PackageTags>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Akka.Streams" Version="$(AkkaVersion)" />
        <PackageReference Include="RabbitMQ.Client" Version="6.4.0" />
    </ItemGroup>
</Project>
