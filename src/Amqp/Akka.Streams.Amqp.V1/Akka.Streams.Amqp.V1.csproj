<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
      <AssemblyTitle>Akka.Streams.Amqp.V1</AssemblyTitle>
      <Description>AMQP V1 adapter for Akka.NET Streams</Description>
      <TargetFrameworks>$(NetStandardLibVersion);$(NetLibVersion)</TargetFrameworks>
      <PackageTags>$(AkkaPackageTags);AMQP;RabbitMQ</PackageTags>
  </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Akka.Streams" Version="$(AkkaVersion)" />
        <PackageReference Include="AMQPNetLite.Core" Version="2.4.5" />
        <PackageReference Include="AMQPNetLite.Serialization" Version="2.4.5" />
    </ItemGroup>

</Project>
