<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyTitle>Akka.Streams.File</AssemblyTitle>
    <Description>The File connectors provide additional connectors for filesystems complementing the sources and sinks for files already included in core Akka.NET Streams</Description>
    <TargetFrameworks>$(NetStandardLibVersion);$(NetLibVersion)</TargetFrameworks>
    <PackageTags>$(AkkaPackageTags);azure;file</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Akka.Streams" Version="$(AkkaVersion)" />
  </ItemGroup>

</Project>
